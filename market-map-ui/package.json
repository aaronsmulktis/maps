  "scripts": {
    "cypress:open": "concurrently \"npm run start:dev\" \"cypress open\"",
    "cypress:run": "cypress run --config baseUrl=http://localhost:3001",
    "cypress:ci:dev": "start-server-and-test start:ci:server:dev http://localhost:3001 cypress:run",
    "cypress:ci:test": "start-server-and-test start:ci:server:test http://localhost:3001 cypress:run",
    "cypress:ci:uat": "start-server-and-test start:ci:server:uat http://localhost:3001 cypress:run",
    "cypress:ci:prod": "start-server-and-test start:ci:server:prod http://localhost:3001 cypress:run",
    "start:ci:server:dev": "npm run build:dev && npm run server",
    "start:ci:server:test": "npm run build:test && npm run server",
    "start:ci:server:uat": "npm run build:uat && npm run server",
    "start:ci:server:prod": "npm run build:prod && npm run server",
    "server": "node build/server.js",
    "lint": "eslint --ext .ts,.tsx src/",
    "sonar_lint": "eslint --ext .js,.ts,.tsx -f ./node_modules/eslint-formatter-sonarqube -o ./linting-results.json --ignore-path .gitignore ./src",
    "test": "cross-env NODE_ENV=test JEST=true jest -c ./jest/jest.config.js",
    "test:watch": "cross-env NODE_ENV=test JEST=true jest -c ./jest/jest.config.js --watch",
    "coverage": "cross-env NODE_ENV=test JEST=true jest -c ./jest/jest.config.js --coverage",
    "prettier": "prettier --config ./prettier.config.js --write \"src/**/*.{js,ts,tsx}\"",
    "start": "cross-env NODE_ENV=development LOCAL=true webpack-dev-server --config ./webpack/webpack.dev.js",
    "start:dev": "cross-env NODE_ENV=development LOCAL=true webpack-dev-server --config ./webpack/webpack.dev.js",
    "start:test": "cross-env NODE_ENV=test LOCAL=true webpack-dev-server --config ./webpack/webpack.dev.js",
    "start:uat": "cross-env NODE_ENV=uat LOCAL=true webpack-dev-server --config ./webpack/webpack.dev.js",
    "start:prod": "cross-env NODE_ENV=production LOCAL=true webpack-dev-server --config ./webpack/webpack.dev.js",
    "build": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.prod.js",
    "build:dev": "cross-env NODE_ENV=development webpack --config ./webpack/webpack.prod.js",
    "build:test": "cross-env NODE_ENV=test webpack --config ./webpack/webpack.prod.js",
    "build:uat": "cross-env NODE_ENV=uat webpack --config ./webpack/webpack.prod.js",
    "build:prod": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.prod.js",
    "build:analyze": "cross-env NODE_ENV=production ANALYZE=true webpack --config ./webpack/webpack.prod.js"
  },
